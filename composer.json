{
    "name": "freema/react-admin-api-bundle",
    "description": "Symfony bundle for React Admin API",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Freema\\ReactAdminApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Freema\\ReactAdminApiBundle\\Dev\\": "dev/",
            "Freema\\ReactAdminApiBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "symfony/http-kernel": "^6.4 || ^7.1",
        "symfony/framework-bundle": "^6.4 || ^7.1",
        "symfony/yaml": "^6.4 || ^7.1",
        "symfony/routing": "^6.4 || ^7.1",
        "symfony/serializer": "^6.4 || ^7.1",
        "symfony/validator": "^6.4 || ^7.1",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.16 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.40",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.5",
        "symfony/debug-bundle": "^6.4 || ^7.1",
        "symfony/monolog-bundle": "^3.10",
        "symfony/browser-kit": "^6.4 || ^7.1",
        "symfony/test-pack": "^1.0"
    },
    "conflict": {
        "symfony/framework-bundle": "<6.4"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "assets": {
                "install-strategy": "copy"
            }
        }
    }
}