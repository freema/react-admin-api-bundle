{
    "name": "vlp/task-worker-bundle",
    "description": "Bundle for manage tasks",
    "type": "library",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "VLM\\TaskWorkerBundleDev\\": "dev/",
            "VLM\\TaskWorkerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VLM\\TaskWorkerBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.1",
        "symfony/http-kernel": "^5.4 || ^6.4 || ^7.1",
        "symfony/framework-bundle": "^5.4 || ^6.4 || ^7.1",
        "symfony/yaml": "^5.4 || ^6.4 || ^7.1",
        "symfony/console": "^5.4 || ^6.4 || ^7.1",
        "dragonmantank/cron-expression": "^3.4",
        "doctrine/doctrine-bundle": "^2.10",
        "doctrine/orm": "^2.16 || ^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.66",
        "phpstan/phpstan": "^1.3",
        "phpunit/phpunit": "^9.5",
        "symfony/debug-bundle": "^5.4 || ^6.4 || ^7.1",
        "symfony/monolog-bundle": "^3.10",
        "symfony/test-pack": "^1.0"
    },
    "conflict": {
        "symfony/framework-bundle": "<5.4"
    },
    "config": {
        "sort-packages": true
    }
}
